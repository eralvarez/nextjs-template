{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate --config ./prisma.config.ts",
    "db:migrate:dev": "prisma migrate dev --config ./prisma.config.ts",
    "db:migrate:deploy": "prisma migrate deploy --config ./prisma.config.ts",
    "db:migrate:status": "prisma migrate status --config ./prisma.config.ts",
    "db:migrate": "prisma migrate deploy --config ./prisma.config.ts",
    "db:reset": "prisma migrate reset --config ./prisma.config.ts --force",
    "db:push": "prisma db push --config ./prisma.config.ts",
    "db:pull": "prisma db pull --config ./prisma.config.ts",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio --config ./prisma.config.ts"
  },
  "dependencies": {
    "@better-auth/cli": "^1.4.7",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
